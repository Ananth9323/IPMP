class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int ptr=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=0){
                nums[ptr++]=nums[i];
            }
        }
        for(;ptr<nums.size();ptr++){
            nums[ptr]=0;
        }
    }
};

link:https://leetcode.com/problems/move-zeroes/description/

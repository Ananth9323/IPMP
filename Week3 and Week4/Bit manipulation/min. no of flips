class Solution {
public:
    int minBitFlips(int start, int goal) {
        int cnt=0;
        while(start||goal){
            if(start%2 | goal%2){
                if(start%2 & goal%2);
                else cnt++;
            }
            start>>=1;
            goal>>=1;
        }
        return cnt;
    }
};

link:https://leetcode.com/problems/minimum-bit-flips-to-convert-number/description/
